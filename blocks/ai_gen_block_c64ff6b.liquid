{% doc %}
  @prompt
    Create a responsive section of circular or square menu options like collection list, add options to add collection from collection list with link image and additional link outside the site with adding image option on menu item, add options to display selecting columns on desktop 1 to 16 and on mobile 1 to 6 in one row with additional show in the below rows if applicable, add option of carousel enable or disable esepcially in mobile with showing minimum 4 in front , add page or full width option, add overlay or below or above menu title and button options separately, design responsive button and menu title with selecting optional title or button, add background color scheme options, add padding options / spacing control options, make it mobile view and desktop view optimized, add lazy load, make SEO friendly, use minimum css, keep it almost static

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-menu-grid-{{ ai_gen_id }} {
    background-color: rgb(var(--color-background));
    padding: {{ block.settings.padding_top }}px 0 {{ block.settings.padding_bottom }}px;
  }

  .ai-menu-grid-container-{{ ai_gen_id }} {
    {% if block.settings.layout == 'page' %}
      max-width: 1200px;
      margin: 0 auto;
      padding: 0 20px;
    {% else %}
      width: 100%;padding: 0 20px;
    {% endif %}
  }

  .ai-menu-grid-wrapper-{{ ai_gen_id }} {
    position: relative;
    overflow: hidden;
  }

  .ai-menu-grid-list-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat({{ block.settings.columns_desktop }}, 1fr);
    gap: {{ block.settings.grid_gap }}px;
    list-style: none;
    margin: 0;
    padding: 0;
    {% if block.settings.enable_carousel %}
      transition: transform 0.3s ease;
    {% endif %}
  }

  .ai-menu-grid-item-{{ ai_gen_id }} {
    position: relative;
    display: flex;
    flex-direction: column;
    text-align: center;}

  .ai-menu-grid-link-{{ ai_gen_id }} {
    display: block;
    text-decoration: none;
    color: inherit;
    transition: transform 0.2s ease;
  }

  .ai-menu-grid-link-{{ ai_gen_id }}:hover {
    transform: translateY(-2px);
  }

  .ai-menu-grid-image-wrapper-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    aspect-ratio: 1;
    margin-bottom: 12px;
    overflow: hidden;{% if block.settings.menu_shape == 'circle' %}
      border-radius: 50%;
    {% else %}
      border-radius: {{ block.settings.border_radius }}px;
    {% endif %}
  }

  .ai-menu-grid-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-menu-grid-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-menu-grid-placeholder-{{ ai_gen_id }} svg {
    width: 40%;
    height: 40%;
    opacity: 0.3;
  }

  .ai-menu-grid-overlay-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, {{ block.settings.overlay_opacity | divided_by: 100.0 }});
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: white;
    opacity: 0;
    transition: opacity 0.3s ease;
  }

  .ai-menu-grid-link-{{ ai_gen_id }}:hover .ai-menu-grid-overlay-{{ ai_gen_id }} {
    opacity: 1;
  }

  .ai-menu-grid-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    font-weight: 600;
    margin: 0 0 8px;
    color: rgb(var(--color-foreground));
  }

  .ai-menu-grid-overlay-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    font-weight: 600;
    margin: 0 0 8px;
    color: white;
  }

  .ai-menu-grid-button-{{ ai_gen_id }} {
    display: inline-block;
    padding: 8px 16px;
    background-color: rgb(var(--color-button));
    color: rgb(var(--color-button-text));
    border: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    text-decoration: none;
    font-size: 14px;
    font-weight: 500;
    transition: background-color 0.2s ease;
  }

  .ai-menu-grid-button-{{ ai_gen_id }}:hover {
    background-color: rgb(var(--color-button-hover));
    color: rgb(var(--color-button-text));
  }

  .ai-menu-grid-overlay-button-{{ ai_gen_id }} {
    display: inline-block;
    padding: 8px 16px;
    background-color: white;
    color: black;
    border: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    text-decoration: none;
    font-size: 14px;
    font-weight: 500;
    transition: all 0.2s ease;
  }

  .ai-menu-grid-overlay-button-{{ ai_gen_id }}:hover {
    background-color: rgba(255, 255, 255, 0.9);
    color: black;
  }

  .ai-menu-grid-carousel-nav-{{ ai_gen_id }} {
    display: none;
    justify-content: center;
    gap: 10px;
    margin-top: 20px;
  }

  .ai-menu-grid-nav-button-{{ ai_gen_id }} {
    width: 40px;
    height: 40px;
    border:1px solid rgb(var(--color-border));
    background: rgb(var(--color-background));
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    transition: all 0.2s ease;
  }

  .ai-menu-grid-nav-button-{{ ai_gen_id }}:hover {
    background: rgb(var(--color-foreground));
    color: rgb(var(--color-background));
  }

  .ai-menu-grid-nav-button-{{ ai_gen_id }}:disabled {
    opacity: 0.3;
    cursor: not-allowed;
  }

  @media screen and (max-width: 749px) {
    .ai-menu-grid-list-{{ ai_gen_id }} {
      grid-template-columns: repeat({{ block.settings.columns_mobile }}, 1fr);
      gap: {{ block.settings.grid_gap | times: 0.8 }}px;
    }

    {% if block.settings.enable_carousel %}
      .ai-menu-grid-wrapper-{{ ai_gen_id }} {
        overflow-x: auto;scrollbar-width: none;
        -ms-overflow-style: none;
      }

      .ai-menu-grid-wrapper-{{ ai_gen_id }}::-webkit-scrollbar {
        display: none;
      }

      .ai-menu-grid-list-{{ ai_gen_id }} {
        grid-template-columns: repeat(auto-fit, minmax(80px, 1fr));
        width: max-content;
        min-width: 100%;
      }

      .ai-menu-grid-carousel-nav-{{ ai_gen_id }} {
        display: flex;
      }
    {% endif %}

    .ai-menu-grid-title-{{ ai_gen_id }},
    .ai-menu-grid-overlay-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.9 }}px;
    }

    .ai-menu-grid-container-{{ ai_gen_id }} {
      padding: 0 15px;
    }
  }

  @media screen and (max-width: 480px) {
    .ai-menu-grid-title-{{ ai_gen_id }},
    .ai-menu-grid-overlay-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.8 }}px;
    }
  }
{% endstyle %}

<div 
  class="ai-menu-grid-{{ ai_gen_id }} color-{{ block.settings.color_scheme }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-menu-grid-container-{{ ai_gen_id }}">
    <menu-grid-{{ ai_gen_id }} class="ai-menu-grid-wrapper-{{ ai_gen_id }}">
      <ul class="ai-menu-grid-list-{{ ai_gen_id }}">
        {% for i in (1..16) %}
          {% liquid
            assign collection_key = 'collection_' | append: i
            assign external_link_key = 'external_link_' | append: i
            assign external_image_key = 'external_image_' | append: i
            assign title_key = 'title_' | append: i
            assign button_text_key = 'button_text_' | append: i
            assign show_title_key = 'show_title_' | append: i
            assign show_button_key = 'show_button_' | append: i

            assign collection = block.settings[collection_key]
            assign external_link = block.settings[external_link_key]
            assign external_image = block.settings[external_image_key]
            assign title = block.settings[title_key]
            assign button_text = block.settings[button_text_key]
            assign show_title = block.settings[show_title_key]
            assign show_button = block.settings[show_button_key]

            assign has_content = false
            if collection != blank or external_link != blank
              assign has_content = true
            endif
          %}

          {% if has_content %}
            <li class="ai-menu-grid-item-{{ ai_gen_id }}">
              {% liquid
                assign link_url = ''
                assign image = null
                assign item_title = ''

                if collection != blank
                  assign link_url = collection.url
                  assign image = collection.featured_image
                  assign item_title = collection.title
                else
                  assign link_url = external_link
                  assign image = external_image
                  assign item_title = title
                endif

                if title != blank
                  assign item_title = title
                endif
              %}

              <a href="{{ link_url }}" class="ai-menu-grid-link-{{ ai_gen_id }}"><div class="ai-menu-grid-image-wrapper-{{ ai_gen_id }}">
                  {% if image %}
                    <img
                      src="{{ image | image_url: width: 300 }}"
                      alt="{{ image.alt | default: item_title | escape }}"
                      class="ai-menu-grid-image-{{ ai_gen_id }}"
                loading="lazy"
                width="300"
                      height="300"
                    >
                  {% else %}
                    <div class="ai-menu-grid-placeholder-{{ ai_gen_id }}">
                      {{ 'collection-1' | placeholder_svg_tag }}
                    </div>
                  {% endif %}

                  {% if block.settings.title_position == 'overlay' or block.settings.button_position == 'overlay' %}
                    <div class="ai-menu-grid-overlay-{{ ai_gen_id }}">
                      {% if block.settings.title_position == 'overlay' and show_title and item_title != blank %}
                        <h3 class="ai-menu-grid-overlay-title-{{ ai_gen_id }}">{{ item_title }}</h3>
                      {% endif %}
                      {% if block.settings.button_position == 'overlay' and show_button and button_text != blank %}
                        <span class="ai-menu-grid-overlay-button-{{ ai_gen_id }}">{{ button_text }}</span>
                      {% endif %}
                    </div>
                  {% endif %}
                </div>

                {% if block.settings.title_position == 'below' and show_title and item_title != blank %}
                  <h3 class="ai-menu-grid-title-{{ ai_gen_id }}">{{ item_title }}</h3>
                {% endif %}

                {% if block.settings.button_position == 'below' and show_button and button_text != blank %}
                  <span class="ai-menu-grid-button-{{ ai_gen_id }}">{{ button_text }}</span>
                {% endif %}
              </a>

              {% if block.settings.title_position == 'above' and show_title and item_title != blank %}
                <h3 class="ai-menu-grid-title-{{ ai_gen_id }}" style="order: -1; margin-bottom: 12px;">{{ item_title }}</h3>
              {% endif %}

              {% if block.settings.button_position == 'above' and show_button and button_text != blank %}
                <a href="{{ link_url }}" class="ai-menu-grid-button-{{ ai_gen_id }}" style="order: -1; margin-bottom: 12px;">{{ button_text }}</a>
              {% endif %}
            </li>
          {% endif %}
        {% endfor %}
      </ul>

      {% if block.settings.enable_carousel %}
        <div class="ai-menu-grid-carousel-nav-{{ ai_gen_id }}">
          <button class="ai-menu-grid-nav-button-{{ ai_gen_id }}" data-direction="prev" aria-label="Previous">
            <svg width="16" height="16" viewBox="0 0 16 16" fill="currentColor">
              <path d="M1012L6 8l4-4v8z"/>
            </svg>
          </button>
          <button class="ai-menu-grid-nav-button-{{ ai_gen_id }}" data-direction="next" aria-label="Next">
            <svg width="16" height="16" viewBox="0 0 16 16" fill="currentColor">
              <path d="M6 4l4 4-4 4V4z"/>
            </svg>
          </button>
        </div>
      {% endif %}
    </menu-grid-{{ ai_gen_id }}>
  </div>
</div>

{% if block.settings.enable_carousel %}
<script>
(function() {
  class MenuGrid{{ ai_gen_id }} extends HTMLElement {
    constructor() {
      super();
      this.currentIndex = 0;
      this.itemsPerView = 4;
    }

    connectedCallback() {
      this.list = this.querySelector('.ai-menu-grid-list-{{ ai_gen_id }}');
      this.items = this.querySelectorAll('.ai-menu-grid-item-{{ ai_gen_id }}');
      this.prevButton = this.querySelector('[data-direction="prev"]');
      this.nextButton = this.querySelector('[data-direction="next"]');

      if (this.items.length <= this.itemsPerView) {
        this.querySelector('.ai-menu-grid-carousel-nav-{{ ai_gen_id }}').style.display = 'none';
        return;
      }

      this.setupEventListeners();
      this.updateButtons();
    }

    setupEventListeners() {
      this.prevButton.addEventListener('click', () => this.slide('prev'));
      this.nextButton.addEventListener('click', () => this.slide('next'));

      let startX = 0;
      let scrollLeft = 0;

      this.addEventListener('touchstart', (e) => {
        startX = e.touches[0].pageX;
        scrollLeft = this.scrollLeft;
      });

      this.addEventListener('touchmove', (e) => {
        const x = e.touches[0].pageX;
        const walk = (x - startX) * 2;
        this.scrollLeft = scrollLeft - walk;
      });
    }

    slide(direction) {
      const maxIndex = Math.max(0, this.items.length - this.itemsPerView);
      
      if (direction === 'next' && this.currentIndex < maxIndex) {
        this.currentIndex++;
      } else if (direction === 'prev' && this.currentIndex > 0) {
        this.currentIndex--;
      }

      const itemWidth = this.items[0].offsetWidth;
      const gap = parseInt(getComputedStyle(this.list).gap);
      const scrollAmount = (itemWidth + gap) * this.currentIndex;
      
      this.scrollTo({
        left: scrollAmount,
        behavior: 'smooth'
      });

      this.updateButtons();
    }

    updateButtons() {
      const maxIndex = Math.max(0, this.items.length - this.itemsPerView);
      this.prevButton.disabled = this.currentIndex === 0;
      this.nextButton.disabled = this.currentIndex >= maxIndex;
    }
  }

  customElements.define('menu-grid-{{ ai_gen_id }}', MenuGrid{{ ai_gen_id }});
})();
</script>
{% endif %}

